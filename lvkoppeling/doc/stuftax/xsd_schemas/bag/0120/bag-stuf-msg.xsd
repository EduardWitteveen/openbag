                                 </xs:sequence>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="openbareRuimte" minOccurs="0" maxOccurs="unbounded">
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="bag-stuf:OpenbareRuimteVraag">
                                 <xs:sequence>
                                    <xs:element name="GerelateerdeWoonplaats" type="bag-stuf:HoortBijWoonplaatsVraag" minOccurs="0"/>
                                 </xs:sequence>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="woonplaats" type="bag-stuf:WoonplaatsVraag" minOccurs="0" maxOccurs="unbounded"/>
                  </xs:choice>
               </xs:complexType>
            </xs:element>
         </xs:sequence>
      </xs:complexType>
