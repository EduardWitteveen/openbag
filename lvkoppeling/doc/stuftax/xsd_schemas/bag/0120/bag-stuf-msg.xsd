   <xs:complexType name="GerelateerdVerblijfsObject">
      <xs:annotation>
         <xs:documentation> Gerelateerd VBO (idem als VBO, maar alleen id is nu verplicht)</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bag-stuf:GerelateerdBagObject">
            <xs:sequence>
