   <xs:complexType name="Pand">
      <xs:annotation>
         <xs:documentation>entiteit Pand</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bag-stuf:BagObject">
            <xs:sequence>
               <xs:element name="pandGeometrie" type="bag:GeometrieVlak" nillable="true"/>
               <xs:element name="bouwjaar" nillable="true">
                  <xs:annotation>
                     <xs:documentation>Oorspronkelijk bouwjaar pand</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:simpleContent>
                        <xs:extension base="bag:Jaar">
                           <xs:attributeGroup ref="StUF:element"/>
                        </xs:extension>
                     </xs:simpleContent>
                  </xs:complexType>
               </xs:element>
               <xs:element name="pandstatus" nillable="true">
                  <xs:complexType>
                     <xs:simpleContent>
                        <xs:extension base="bag:StatusPand">
                           <xs:attributeGroup ref="StUF:element"/>
                        </xs:extension>
                     </xs:simpleContent>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
            <xs:attributeGroup ref="StUF:fundamenteel"/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
