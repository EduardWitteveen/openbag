                                    <xs:element name="GerelateerdeVerblijfsObject" type="bag-stuf:BevatVerblijfsObjectVraag" minOccurs="0" maxOccurs="unbounded"/>
                                 </xs:sequence>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="verblijfsObject" minOccurs="0" maxOccurs="unbounded">
