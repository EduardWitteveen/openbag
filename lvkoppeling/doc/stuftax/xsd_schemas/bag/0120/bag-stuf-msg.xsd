                                 <xs:sequence>
                                    <xs:element name="GerelateerdeAdressen" type="bag-stuf:HeeftNummerAanduidingVraag" minOccurs="0"/>
                                    <xs:element name="GerelateerdPand" type="bag-stuf:LigtInPandVraag" minOccurs="0"/>
                                 </xs:sequence>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="ligplaats" minOccurs="0" maxOccurs="unbounded">
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="bag-stuf:LigplaatsVraag">
                                 <xs:sequence>
                                    <xs:element name="GerelateerdeAdressen" type="bag-stuf:HeeftNummerAanduidingVraag" minOccurs="0"/>
                                 </xs:sequence>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="standplaats" minOccurs="0" maxOccurs="unbounded">
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="bag-stuf:StandplaatsVraag">
                                 <xs:sequence>
                                    <xs:element name="GerelateerdeAdressen" type="bag-stuf:HeeftNummerAanduidingVraag" minOccurs="0"/>
                                 </xs:sequence>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="nummerAanduiding" minOccurs="0" maxOccurs="unbounded">
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="bag-stuf:NummerAanduidingVraag">
                                 <xs:sequence>
                                    <xs:element name="GerelateerdeWoonplaats" type="bag-stuf:HoortBijWoonplaatsVraag" minOccurs="0" maxOccurs="unbounded"/>
